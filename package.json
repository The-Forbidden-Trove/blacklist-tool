{
  "name": "tft-blacklist",
  "version": "1.0.3",
  "main": "app.js",
  "productName": "TFT Blacklist",
  "scripts": {
    "start": "electron .",
    "start-poe2": "cross-env poe2_mode=true electron .",
    "prebuild": "npm install && npm install --only=dev",
    "build": "echo process.env.poe2_mode = 'false'; > poever.js && electron-packager . --icon views/tft.ico --overwrite",
    "build-poe2": "rimraf --glob 'TFT Blacklist-win32-x64/*' && npm run prebuild && echo process.env.poe2_mode = 'true'; > poever.js && electron-packager . TFT-Blacklist-poe2 --icon views/tft.ico --overwrite"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cross-env": "^7.0.3",
    "electron-overlay-window": "^2.0.1",
    "jquery": "^3.6.0",
    "sane": "^5.0.1",
    "toastr": "^2.1.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "electron": "^13.1.6",
    "electron-packager": "^15.3.0"
  }
}
